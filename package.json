{
  "name": "arvi",
  "version": "1.0.0",
  "description": "Professional arvi - Full Stack Application",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel --filter=web --filter=api",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=api",
    "dev:mobile": "turbo run dev --parallel --filter=mobile --filter=api -- --tunnel",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "turbo": "^1.10.16",
    "@types/node": "^20.8.7",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3",
    "eslint": "^8.52.0"
  },
  "packageManager": "npm@10.0.0"
}
